pipeline{
    agent ant
    stages{
        stage('scm'){
            steps{
                 sh 'checkout' git url ''
            }
            
        }
        stage('build'){
            steps{
                sh 'npm install'
                sh 'npm run build'

                }
        }
        stage('test'){
            steps{
                sh 'npm run test'
            }
        }
    }
}